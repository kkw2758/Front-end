package org.comstudy.myapp.saram.model;

public class SaramDTO {
	private int idx;
	private String id;
	private String name;
	private int age;

	// 생성자 Constructor
	public SaramDTO(int idx, String id, String name, int age) {
		super();
		this.idx = idx;
		this.id = id;
		this.name = name;
		this.age = age;
	}

// 겟터 & 셋터 만들기
	public int getIdx() {
		return idx;
	}

	public void setIdx(int idx) {
		this.idx = idx;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

}
